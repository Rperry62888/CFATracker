@model IEnumerable<CFAStudentTracker.Models.UserQueue_Result>

@{
    ViewBag.Title = "Pending Files";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">@ViewBag.Title.</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="row">
    <div class="col-lg-12">
        <table class="table table-striped table-bordered table-hover">
            <thead>
                <tr>
                    <th>#</th>
                    <th>@Html.DisplayNameFor(model => model.SSN)</th>
                    <th>@Html.DisplayNameFor(model => model.Name)</th>
                    <th>@Html.DisplayNameFor(model => model.Type)</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td class="col-sm-1">@Html.ActionLink("Open", "Edit", new { id = item.ID }, htmlAttributes: new { @class = "btn btn-primary" })</td>
                        <td>@Html.DisplayFor(modelItem => item.SSN)</td>
                        <td>@Html.DisplayFor(modelItem => item.Name)</td>
                        <td>@Html.DisplayFor(modelItem => item.Type)</td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
    <!-- /.col-lg-12 -->
</div>

  

